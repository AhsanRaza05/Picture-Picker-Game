<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
   <children>
      <ImageView id="background" fitHeight="800.0" fitWidth="1297.0" pickOnBounds="true">
         <image>
            <Image url="@../../images/picturePickerBackground.jpg" />
         </image>
      </ImageView>
      <ButtonBar layoutX="549.0" layoutY="589.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button id="startButton" fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#init4Square" prefHeight="85.0" prefWidth="200.0" style="-fx-border-color: #028800; -fx-background-color: #3d2814;" text="START!" textAlignment="CENTER" textFill="#028800">
               <font>
                  <Font name="Arial Black" size="34.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
        </buttons>
      </ButtonBar>
      <Label layoutX="264.0" layoutY="177.0" text="PICTURE PICKER" textAlignment="CENTER" textFill="#028800">
         <font>
            <Font name="Arial Black" size="81.0" />
         </font>
         <effect>
            <DropShadow height="3.0" offsetX="3.0" offsetY="3.0" radius="1.0" spread="1.0" width="3.0" />
         </effect>
      </Label>
   </children>
